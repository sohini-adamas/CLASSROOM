#include<stdio.h>
int main(){
    int n1,n2,divsum1,divsum2;
    printf("the numbers are: ");
    scanf("%d %d",&n1,&n2);
    for(int i=1;i<n1;i++){
        if(n1%i==0){            
            divsum1=divsum1+i;
        }
    }
    for(int i=1;i<n2;i++){
        if(n2%i==0){
            divsum2=divsum2+i;
        }
    }
    if(n1==divsum2 && n2==divsum1){
        printf("amicable");
    }
    else{
        printf("not amicable");
    }
}
